# Appfile for CyberVPN iOS
# This file contains app-specific configuration for Fastlane

# Your Apple Developer account email
apple_id ENV["APPLE_ID"] || "your-apple-id@example.com"

# Your Team ID from Apple Developer Portal
team_id ENV["TEAM_ID"] || "YOUR_TEAM_ID"

# iTunes Connect Team ID (if different from Developer Portal Team ID)
itc_team_id ENV["ITC_TEAM_ID"] || ENV["TEAM_ID"]

# Bundle identifiers for each flavor
app_identifier({
  dev: "com.cybervpn.app.dev",
  staging: "com.cybervpn.app.staging",
  production: "com.cybervpn.app"
})

# For more information about the Appfile, see:
# https://docs.fastlane.tools/advanced/#appfile
